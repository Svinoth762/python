a=int(input("enter a:"))
b=int(input("enter b:"))
choice=int(input("enter choice(1=add,2=sub,3=mul,4=div):"))
if choice==1:
    sum=a+b
    print("addition of two numbers is:",sum)
elif choice==2:
    sub=a-b
    print("subtraction of two numbers is:",sub)
elif choice==3:
    mul=a*b
    print("multiplication of two numbers is:",mul)
elif choice==4:
    div=a/b
    print("division of two numbers is:",div)
    
